  set from      = "********@riseup.net"
  set sendmail  = "/usr/local/bin/msmtp -a riseup"
  set mbox      = "+riseup/inbox"
  unset record
  set postponed     = "+riseup/draft"
  set signature     = ~/.mutt/accounts/riseup_sig
  set pgp_sign_as=****************

color status blue default

