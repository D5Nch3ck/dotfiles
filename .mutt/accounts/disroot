  set from      = "d5nch3ck@disroot.org"
  set sendmail  = "/usr/local/bin/msmtp -a disroot"
  set mbox      = "+disroot/all_mail"
  unset record
  set postponed     = "+disroot/drafts"
  set signature     = ~/.mutt/accounts/disroot_sig
  set pgp_sign_as=0x0719E8872FD74053

color status green default

